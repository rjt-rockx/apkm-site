{
	"name": "apkm-site",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"fix": "prettier --write . && eslint . --fix --plugin file-progress --rule \"file-progress/activate: 1\""
	},
	"devDependencies": {
		"@iconify-json/mdi": "^1.1.66",
		"@sveltejs/adapter-auto": "^3.2.0",
		"@sveltejs/enhanced-img": "^0.2.0",
		"@sveltejs/kit": "^2.5.8",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@tailwindcss/typography": "^0.5.10",
		"@types/eslint": "^8.56.10",
		"@typescript-eslint/eslint-plugin": "^7.2.0",
		"@typescript-eslint/parser": "^7.2.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-file-progress": "^1.3.0",
		"eslint-plugin-simple-import-sort": "^12.1.0",
		"eslint-plugin-svelte": "^2.39.0",
		"postcss": "^8.4.33",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.3",
		"prettier-plugin-tailwindcss": "^0.5.1",
		"svelte": "^4.2.17",
		"svelte-check": "^3.7.1",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.5",
		"unplugin-icons": "^0.19.0",
		"vite": "^5.2.11"
	},
	"type": "module",
	"dependencies": {
		"@fontsource-variable/roboto-flex": "^5.0.15",
		"@fontsource-variable/roboto-mono": "^5.0.19",
		"@fontsource/roboto": "^5.0.13",
		"@iconify-json/simple-icons": "^1.1.101",
		"bits-ui": "^0.21.7",
		"tailwind-merge": "^2.3.0"
	}
}
